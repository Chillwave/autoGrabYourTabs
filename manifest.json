{
    "manifest_version": 3,
    "name": "YouTube Link Grabber",
    "version": "1.0",
    "description": "Fetches YouTube video links from all open tabs.",
    "permissions": ["tabs", "downloads"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png"
    }
  }
  